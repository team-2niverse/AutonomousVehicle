<?xml version="1.0" encoding="UTF-8"?>
<launch>
	<node pkg="joy" exec="joy_node">
		<param name="deadzone" value="0.08"/>
	</node>
	<node pkg="planner_manager" exec="planner_manager_node"/>
	<node pkg="ssv_control" exec="ssv_control_node"/>
	<node pkg="camera_ros" exec="camera_node">
		<param name="format" value="BGR888"/>
		<param name="sensor_mode" value="3280:2464"/>
		<param name="width" value="160"/>
		<param name="height" value="120"/>
		<param name="orientation" value="180"/>
	</node>

	<include file="$(find-pkg-share ros2_socketcan)/launch/socket_can_bridge.launch.xml"/>
</launch>
